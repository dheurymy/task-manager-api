{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/server.js" }
    ], "functions": [{
      "include": "/api/*",
      "exclude": ["/api/not-allowed/*"],
      "function": {
        "maxDuration": 60
      }
    }]
  }
  